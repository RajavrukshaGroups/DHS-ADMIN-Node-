<script language="javascript" type="text/javascript">
  function printDiv(divID) {
    // Get the HTML of the div
    var divElements = document.getElementById(divID).innerHTML;
    // Store the original HTML of the page
    var originalContent = document.body.innerHTML;

    // Replace the body's HTML with the div's HTML
    document.body.innerHTML = divElements;

    // Trigger the print dialog
    window.print();

    // Restore the original HTML of the page
    document.body.innerHTML = originalContent;
}
</script>  

<script>


    function goBack() {
        window.history.back();
    }


    function printDiv(divID) {
        var divElements = document.getElementById(divID).innerHTML;
        var oldPage = document.body.innerHTML;
        document.body.innerHTML = "<html><head><title></title></head><body>" + divElements + "</body>";
        window.print();
        document.body.innerHTML = oldPage;
    }

    document.addEventListener('DOMContentLoaded', () => {
        const urlParams = new URLSearchParams(window.location.search);
        const user_pk = urlParams.get('user_pk');
        console.log("This is pk : ", user_pk)
        fetch(`/gen_share_view?user_pk=${user_pk}`)
            .then(response => response.json())
            .then(data => {
                if (data.result && data.result.length > 0) {
                    console.log("Inside the function")
                    const membername = data.result[0].username
                    const add = data.result[0].user_address
                    const date = data.result[0].created_date
                    const formattedDate = new Date(date).toLocaleDateString('en-GB');
                    const nomineename = data.result[0].nominess_name
                    const nomineerelation = data.result[0].nominess_relationship
                    const share_cert_no = data.result[0].share_cert_no

                    console.log(formattedDate);
                    console.log("This is membername : ", membername)
                    document.getElementById('memberNamePlaceholder').textContent = membername;
                    document.getElementById('address').textContent = add
                    document.getElementById('dt').textContent = formattedDate
                    document.getElementById('nomine_name').textContent = nomineename
                    document.getElementById('nomineerel').textContent = nomineerelation
                    document.getElementById('sno').textContent = share_cert_no
                }
            }
            ).catch(error => {
                console.error('Error fetching data:', error);
            })

    })

</script>
