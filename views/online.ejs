                 document.getElementById('sitePaymentInfoText').textContent = sitePaymentInfoText;
                 document.getElementById('sitepurchasebank').textContent = result.site_payment_bank;
                 document.getElementById('sitepurchasebranch').textContent = result.site_payment_branch;
 
