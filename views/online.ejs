<script>
    // Function to get query parameters from the URL
    function getQueryParam(param) {
         const urlParams = new URLSearchParams(window.location.search);
         return urlParams.get(param);
     }
 
     // Extract 'id' from the URL
     const id = getQueryParam('id');
     // If 'id' is found in the URL, fetch the data
     if (id) {
         fetch(`/download/fetchOnlineApplication?id=${id}`) // Use backticks for template literals
             .then(response => response.json())
             .then(data => {
                 if (data.error) {
                     console.error(data.error);
                     return;
                 }
                 
                 const result = data.result[0];  // Fetch the first result
                 
                 // Populate fields with fetched data
                 document.getElementById('profile-image').src = `data:image/png;base64,${result.image}`;
                 document.getElementById('signimage').src = `data:image/png;base64,${result.signature}`;
                //  document.getElementById('name').textContent = result.name;
                document.getElementById('name').textContent = result.name.toUpperCase();

                 document.getElementById('name1').textContent = result.name;
                 document.getElementById('designation').textContent = result.ref_designation;
                 document.getElementById('serviceid').textContent = result.ref_id; 
                 document.getElementById('relationship').textContent = result.ref_relationship;
                 document.getElementById('property_size').textContent = result.property_size;
                 document.getElementById('property_name').textContent = result.property_name;
                 document.getElementById('propertylocation').textContent = result.property_location;
 
                 const dateObj = new Date(result.dob);
                 const formattedDob = `${dateObj.getDate().toString().padStart(2, '0')}-${(dateObj.getMonth() + 1).toString().padStart(2, '0')}-${dateObj.getFullYear()}`;
                 document.getElementById('dob').textContent = formattedDob;
 
                 document.getElementById('age').textContent = result.age;
                 document.getElementById('fatherSpouseNames').textContent = result.father_spouse_name;
                 document.getElementById('correspondence').textContent = result.correspondence_address;
                 document.getElementById('permanent').textContent = result.permanent_address;
                 document.getElementById('workingaddress').textContent = result.working_address;
                 document.getElementById('nominename').textContent = result.nominee_name;
                 document.getElementById('nage').textContent = result.nominee_age;
                 document.getElementById('nominerelation').textContent = result.nominee_relationship;
                 document.getElementById('nominee_address').textContent = result.nominee_address;
                 document.getElementById('remarks').textContent = result.remarks;
                 document.getElementById('phone').textContent = result.phone;
                 document.getElementById('mobile').textContent = result.phone;
                 document.getElementById('email').textContent = result.email;
 
                 const paymentInfoText = `${result.membership_payment_mode.charAt(0).toUpperCase() + result.membership_payment_mode.slice(1)} : ${result.membership_payment_reference_no}`;
                 document.getElementById('payment-info').textContent = paymentInfoText;
                 document.getElementById('bank').textContent = result.membership_payment_bank;
                 document.getElementById('branch').textContent = result.membership_payment_branch;
                 
                 const formattedAdvance = `Rs.${new Intl.NumberFormat('en-IN').format(result.site_payment_amount)}/-`;
                 document.getElementById('advance').textContent = formattedAdvance;
                 document.getElementById('site_payment_amount_in_words').textContent = result.site_payment_amount_in_words;
 
